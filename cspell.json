{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "words": [
    "cacheable",
    "commitlint",
    "compodoc",
    "devkit",
    "dtos",
    "Elsayed",
    "haptics",
    "homecomponent",
    "ionicons",
    "nestjs",
    "ngcc",
    "nitrosystem",
    "nrwl",
    "servicescomponent",
    "sonarjs",
    "Sonarqube",
    "storysource",
    "stylelint",
    "tailwindcss",
    "titlecase"
  ],
  "language": "en,ar",
  "ignoreRegExpList": ["/[ุก-ู]+/"],
  "files": ["**"],
  "enableGlobDot": true,
  "ignorePaths": [
    "node_modules",
    "dist",
    ".git",
    ".angular",
    ".gitignore",
    ".husky/_/**",
    ".eslintrc.json",
    "**/*.svg",
    "decorate-angular-cli.js",
    ".browserslistrc",
    "cspell.json",
    ".vscode/**",
    "jest.config.ts",
    "android",
    "ios",
    ".scannerwork",
    "nginx.conf"
  ]
}
