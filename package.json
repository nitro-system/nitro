  "scripts": {
    "build": "npx nx run-many --target=build --all --parallel=false",
    "cap:add": "cross-env project=$npm_config_project npx cap add",
    "cap:open": "cross-env project=$npm_config_project npx cap open",
    "cap:sync": "cross-env project=$npm_config_project npx cap sync",
    "check:all": "npm-run-all check:test check:e2e check:lint build check:cspell check:format check:style",
    "check:cspell": "npx cspell --no-progress --show-suggestions --show-context",
    "check:e2e": "npx nx run-many --target=e2e --all --parallel=false",
    "check:format": "npx prettier . --check",
    "check:lint": "npx eslint . && npx nx workspace-lint && npx nx run-many --target=lint --all",
    "check:style": "npx stylelint \"{apps,libs}/**/*.{css,less,sass,scss,sss}\"",
    "check:test": "npx nx run-many --target=test --all",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "ng": "nx",
    "prepare": "husky install",
    "start": "npx nx run-many --target=serve --all --parallel=false"
  },
  "prettier": "@js-omar/prettier-config",
