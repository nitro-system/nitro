{
  "projectType": "application",
  "sourceRoot": "apps/m-admin/src",
  "prefix": "m-admin",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/m-admin",
        "index": "apps/m-admin/src/index.html",
        "main": "apps/m-admin/src/main.ts",
        "polyfills": "apps/m-admin/src/polyfills.ts",
        "tsConfig": "apps/m-admin/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/m-admin/src/favicon.ico",
          "apps/m-admin/src/assets",
          {
            "glob": "**/*.svg",
            "input": "node_modules/ionicons/dist/ionicons/svg",
            "output": "./svg"
          }
        ],
        "styles": [
          "apps/m-admin/src/theme/variables.scss",
          "apps/m-admin/src/styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/m-admin/src/environments/environment.ts",
              "with": "apps/m-admin/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "m-admin:build:production"
        },
        "development": {
          "browserTarget": "m-admin:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "port": 4002,
        "proxyConfig": "apps/m-admin/proxy.conf.json"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "m-admin:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/m-admin/**/*.ts", "apps/m-admin/**/*.html"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/m-admin"],
      "options": {
        "jestConfig": "apps/m-admin/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["mobile"]
}
